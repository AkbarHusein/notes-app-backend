{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["google"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-unused-vars": "off",
    "linebreak-style": "off",
    "quotes": ["error", "double"],
    "object-curly-spacing": ["error", "always"],
    "indent": ["error", 2],
    "comma-dangle": "off"
  }
}
